<doctrine-mapping>
    <entity name="VideoLibrary\Api\Infrastructure\Persistence\Doctrine\Entity\Subtitle" table="subtitle">
        <id name="id" type="guid" />
        <field name="language" type="string" length="2" />
        <many-to-one field="video" target-entity="Video"></many-to-one>
    </entity>
</doctrine-mapping>